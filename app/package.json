{
  "name": "expo-starter-kit-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios --device",
    "android": "expo run:android",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "postinstall": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "audit": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-camera-roll/camera-roll": "^7.10.2",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@reactvision/react-viro": "^2.43.3",
    "base64-arraybuffer": "^1.0.2",
    "color-namer": "^1.4.0",
    "expo": "~52.0.43",
    "expo-constants": "~17.0.8",
    "expo-file-system": "~18.0.12",
    "expo-image-manipulator": "~13.0.6",
    "expo-linking": "~7.0.5",
    "expo-media-library": "^18.2.0",
    "expo-router": "~4.0.21",
    "expo-system-ui": "^6.0.9",
    "react": "18.3.1",
    "react-native": "0.83.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-paper": "^5.14.5",
    "react-native-permissions": "^5.4.2",
    "react-native-pixel-color": "^0.0.5",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-view-shot": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^19.1.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.3.12",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.1.3"
  },
  "expo": {
    "entryPoint": "index.js",
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@reactvision/react-viro"
        ]
      }
    }
  },
  "private": true
}
